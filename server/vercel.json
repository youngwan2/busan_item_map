{
    "builds": [
        {
            "src": "server.cjs",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "server.cjs",
          "methods":["POST"],
          "headers": {
            "Content-Length":"450MB"
          }
        }
    ]
  }